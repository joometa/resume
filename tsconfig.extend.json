{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@src": [
        "./src"
      ],
      "@src/*": [
        "./src/*"
      ],
      "@components": [
        "./src/components"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@core": [
        "./src/core"
      ],
      "@core/*": [
        "./src/core/*"
      ]
    }
  }
}